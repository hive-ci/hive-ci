<div class="panel panel-primary">
  <div class="panel-heading">
    <h3>Pass/Fail/Error for project</h3>
  </div>

  <div class="panel-body">
    <svg class="project-state-chart" width="100%" height="400"></svg>
  </div>
</div>

<script>
  d3.json("<%= job_status_project_path(params[:project_id]) %>.json", function(error, json) {
    if (error) return console.warn(error);
    renderD3LineChart('.project-state-chart', json);
  });
</script>
